name = MoPub

# The location of the flex sdk with Air 3.1 or later
sdk.home = /path/to/your/flexsdk/folder

# Set Java home to 1.6 - /usr/libexec/java_home  -V
java.home = /System/Library/Java/JavaVirtualMachines/1.6.0.jdk/Contents/Home

# mopub-client sdk - this should be the version at https://github.com/sticksports/mopub-client
mopub.sdk = /path/to/mopub-client

# AdMob android client sdk
admob.android.jar = /path/to/admob/android/sdk/jar

# Millennial Media android client SDK
millennial.android.jar = /path/to/millennial-media/android/sdk/jar

# Chartboost android client SDK
chartboost.android.jar = /path/to/chartboost/android/sdk/jar

# The location of the .p12 certificate file
# This is not your apple developer certificate. It is a certificate for Air,
# which may be a self-signed certificate created with adt
keystore.file = /path/to/your/certificate/selfSignedTestCert.p12

# The password of the .p12 certificate file
keystore.password = selfSignedTestPassword

# Location of the android sdk
android.adk = path/to/your/android-sdk/platforms/android-8

# Location of the iOS 6 SDK
ios.sdk = path/to/iPhoneOS6.0.sdk
